			"name": "Add Employee",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"var jsonData = pm.response.json();",
							"pm.environment.set(\"id\", jsonData.id);",
							"",
							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"// Check if First and Last names are correct",
							"pm.test(\"First name is correct\", function() {",
							"    pm.expect(jsonData.firstName).to.eql(\"New\");",
							"});",
							"pm.test(\"Last name is correct\", function() {",
							"    pm.expect(jsonData.lastName).to.eql(\"Employee\");",
							"});",
							"",
							"//Check the dependants",
							"pm.test(\"Dependants number is correct\", function() {",
							"    pm.expect(jsonData.dependants).to.eql(2);",
							"});",
							"",
							"//Check Benefits Cost is correct",
							"pm.test(\"Benefits Cost is correct\", function() {",
							"    pm.expect(jsonData.benefitsCost).to.eql(76.92308);",
							"});",
							"",
							"// Make sure gross is 2000",
							"pm.test(\"Gross is correct\", function() {",
							"    pm.expect(jsonData.gross).to.eql(2000)",
							"});",
							"",
							"// Confirm that salary is 52000",
							"pm.test(\"Salary is correct\", function() {",
							"    pm.expect(jsonData.salary).to.eql(52000);",
							"})",
							"",
							"// Check Net pay is correct",
							"pm.test(\"Net pay is correct\", function() {",
							"    pm.expect(jsonData.net).to.eql(1923.0769);",
							"});"
						],
						"type": "text/javascript",
